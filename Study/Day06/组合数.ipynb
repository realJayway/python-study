    "\"\"\"\n",
    "计算C(M,N)\n",
    "C(M,N)=M!/N!(M-N)！\n",
    "\"\"\"\n",
    "\n",
    "\n",
    "m=int(input('m='))\n",
    "n=int(input('n='))\n",
    "fm=1\n",
    "for x in range(1,m+1):\n",
    "    fm*=x\n",
    "fn=1\n",
    "for y in range(1,n+1):\n",
    "    fn*=y\n",
    "fmn=1\n",
    "for z in range(1,m-n+1):\n",
    "    fmn*=z\n",
    "print('C(%d,%d)=%d'%(m,n,fm//fn//fmn))"
